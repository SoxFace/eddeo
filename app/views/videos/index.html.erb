  <li><a class="fancybox fancybox.iframe" href="http://player.vimeo.com/video/63534746"><img src="https://i.vimeocdn.com/video/217554055_200x150.jpg" alt=""></a></li>
  
  <a class="fancybox fancybox.iframe" href="http://player.vimeo.com/video/63534746">Vimeo</a></li>

  <script>
    $(document).ready(function() {
      $(".fancybox").fancybox({
          'href' : $('.fancybox').attr('ul'),
          type: "iframe"
      });
    });
  </script>