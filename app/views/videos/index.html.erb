<h1>This is the Video Landing Page</h1>

  <h3>Upper Limb Positioning Images</h3>
  <ul>
    <% upper_limb %>
    <% @videos.each do |video| %>
      <li>
        <a class="fancybox fancybox.iframe" href="//player.vimeo.com/video/<%= video.try :vimeo_id %>?portrait=0&color=333"><img src="https://i.vimeocdn.com/video/<%= video.try :pictures %>_200x150.jpg" alt=""></a>
        <%= video.try :name %>
      </li> 
    <% end %>
  </ul>

  <script>
    $(document).ready(function() {
      $(".fancybox").fancybox({
          'href' : $('.fancybox').attr('ul'),
          type: "iframe"
      });
    });
  </script>