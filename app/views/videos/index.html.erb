<h1>This is the Video Landing Page</h1>

  <h3>Upper Limb Positioning Vimeo</h3>
  <ul>
    <% upper_limb %>
    <% @videos.each do |video| %>
      <li>
        <iframe src="//player.vimeo.com/video/<%= video.try :vimeo_id %>?portrait=0&color=333" width="WIDTH" height="HEIGHT" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe><img src="https://i.vimeocdn.com/video/<%= video.try :pictures %>_200x150.jpg" alt="">
        <%= video.try :name %>
      </li> 
    <% end %>
  </ul>

  <h3>Upper Limb Positioning Images</h3>
  <ul>
    <% upper_limb %>
    <% @videos.each do |video| %>
      <li>
        <a class="fancybox fancybox.iframe" href="//player.vimeo.com/video/<%= video.try :vimeo_id %>?portrait=0&color=333"><img src="https://i.vimeocdn.com/video/<%= video.try :pictures %>_200x150.jpg" alt=""></a>
        <%= video.try :name %>
      </li> 
    <% end %>
  </ul>


  <div class="fancybox"></div>
